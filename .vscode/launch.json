    "version": "0.2.0",
    "configurations": [
    
        {
            "name": "gateway",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceRoot}/apps/gateway",
            "cwd": "${workspaceRoot}/apps/gateway",
            "env": {
                "GO111MODULE": "on",
                "GOPROXY": "https://goproxy.cn,direct"
            },
            "args": []
        },
        {
            "name": "user",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceRoot}/apps/user/cmd/user-service/main.go",
            "cwd": "${workspaceRoot}/apps/user/cmd/user-service",
            "env": {
                "GO111MODULE": "on",
                "GOPROXY": "https://goproxy.cn,direct"
            },
            "args": [
                "--config",
                "${workspaceRoot}/apps/user/config.yaml"
            ]
        }
    ]
}